{
    "compileOnSave": true,
    "compilerOptions": {
        "target": "ES2020",
        "module": "ES2015",
        "moduleResolution": "node",
        "strict": true,
        "resolveJsonModule": true,
        "experimentalDecorators": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "strictNullChecks": true,
        "outDir": "js/"
    }
}